<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class name="agencia.modelo.OfertaVuelo" table="ofertas_vuelos" catalog="agencia">
      
      <id name="refOfertaVuelo" column="REF_OFERTA_VUELO" type="integer">
          <generator class="native" />
      </id>
      
      <property name="refVuelo" column="REF_HOTEL" type="integer" />
      <property name="preTur" column="PRE_TURISTA" type="double" />
      <property name="preBuss" column="PRE_BUSSINES" type="double" />
      <property name="preEco" column="PRE_ECONOMICO" type="double" />
      
  </class>

</hibernate-mapping>

